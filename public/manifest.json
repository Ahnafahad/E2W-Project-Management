{
  "name": "E2W Project Management",
  "short_name": "E2W PM",
  "description": "Professional project management tool by E2W Global",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFFFFF",
  "theme_color": "#000000",
  "orientation": "portrait-primary",
  "categories": ["productivity", "business", "utilities"],
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "icons": [
